@html.extend("layout", function(model){
	@html.block("body", function(model){
		<div class="jumbotron">
			<h1>@model.title</h1>
		</div>
		 
		@model.posts.forEach(function(item){
				<div class="list-group">
					<a href="@item.text" class="list-group-item active">
			    		@item.text
			    	</a>
			    </div>
		})

	})
})