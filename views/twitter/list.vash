@html.extend("layout", function(model){
	@html.block("body", function(model){
		<div class="jumbotron">
		
			<h1>@model.title</h1>
		
		</div>
		 
		@model.posts.forEach(function(item){
			@if(item){
				<div class="list-group">
					<a href="#" class="list-group-item active">
			    		@item.text
			    	</a>
			    </div>
			}
		})

	})
})