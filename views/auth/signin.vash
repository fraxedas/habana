
<div class="container">   
    <div class="row">
        <div class="col-sm-6 col-md-4 col-md-offset-4">
            @model.providers.forEach(function(item){
                <div class="list-group ">
                    <a href="@item.url" class="btn btn-lg btn-primary btn-block">
                        @item.name
                    </a>
                </div>	
            })
        </div>
    </div>

</div>

